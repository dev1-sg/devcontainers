{
  "id": "golang",
  "version": "1.0.1",
  "name": "golang",
  "description": "",
  "documentationURL": "https://github.com/dev1-sg/devcontainers/blob/main/src/features/golang/README.md",
  "containerEnv": {
    "GOROOT": "/usr/local/go",
    "PATH": "/usr/local/go/bin:${PATH}"
