{
  "id": "docker-outside-of-docker",
  "version": "1.0.1",
  "name": "docker-outside-of-docker",
  "description": "",
  "documentationURL": "https://github.com/dev1-sg/devcontainers/blob/main/src/features/docker-outside-of-docker/README.md",
  "privileged": true,
   "containerEnv": {
      "DOCKER_BUILDKIT": "1"
  },
  "mounts": [
    {
      "source": "/var/run/docker.sock",
      "target": "/var/run/docker.sock",
      "type": "bind"
    }
  ],
  "customizations": {
    "vscode": {
        "extensions": ["ms-azuretools.vscode-docker"]
    }
  }
}
