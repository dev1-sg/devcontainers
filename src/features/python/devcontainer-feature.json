{
  "id": "python",
  "version": "1.0.0",
  "name": "python",
  "description": "",
  "documentationURL": "https://github.com/dev1-sg/devcontainers/blob/main/src/features/python/README.md",
  "containerEnv": {
    "POETRY_HOME": "/opt/poetry",
    "POETRY_VENV": "/opt/poetry/venv",
    "POETRY_CACHE_DIR": "/opt/poetry/cache",
    "POETRY_NO_INTERACTION": "1",
    "POETRY_VIRTUALENVS_CREATE": "false",
    "PATH": "/opt/poetry/bin:${PATH}"
  },
  "options": {
    "version": {
        "type": "string",
        "proposals": [
            "3.13.3"
        ],
        "default": "3.13.3",
        "description": "Select or enter an Python version."
    }
  }
}
