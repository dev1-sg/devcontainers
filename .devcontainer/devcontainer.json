{
  "image": "public.ecr.aws/f7i0q1v8/devcontainer/ubuntu-base:jammy",
  "name": "docker-base-images",
  "features": {
      "ghcr.io/dev1-sg/devcontainers/common-utils:1": {},
      "ghcr.io/dev1-sg/devcontainers/docker-in-docker:latest": {},
      "ghcr.io/dev1-sg/devcontainers/github-cli:1": {},
      "ghcr.io/dev1-sg/devcontainers/go:1": {}
  },
  "customizations": {
    "vscode": {
      "settings": { "terminal.integrated.defaultProfile.linux": "zsh" },
    "extensions": []
    }
  },
  "containerUser": "vscode",
  "workspaceMount": "source=${localWorkspaceFolder}/,target=/home/vscode/docker-base-images,type=bind,consistency=cached", "workspaceFolder": "/home/vscode",
  "postCreateCommand": "sudo chown -R vscode:vscode /home/vscode"
}
